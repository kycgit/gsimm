    \ \ \ \ \ \ (setf (tpsy-t ndt) (+ (tpsy-t idt) nts))

    \ \ \ \ \ \ (setf (pobj-a j) ag)

    \ \ \ \ \ \ (setf (pobj-r j) [pobj-r j pobj-v j .+ .* nts 0.5 .* pobj-a j
    nts .+ .* ^ 2])

    \ \ \ \ \ \ (setf (pobj-v j) [pobj-v j pobj-a j .+ .* nts ])

    \ \ \ \ \ \ ndt)))

