    (pobj-r (cadr (tpsy-psy x))) 1)))

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ [sqrt +(rx ^ 2 ry ^
